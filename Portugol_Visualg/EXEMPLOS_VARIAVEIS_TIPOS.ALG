Algoritmo "Exemplos_Variaveis_tipos"

Var

   x :inteiro
   y: real


Inicio

//  Y <- 2.5
//  x <- 2 * y neste caso x é inteiro não pode receber valor real mais real pode receber numero inteiro

     x <- 5
     y <- 2 * x
      
      escreval (x)
      escreval (y)


Fimalgoritmo